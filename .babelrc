{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "array-includes",
    "syntax-class-properties",
    "syntax-decorators",
    "syntax-object-rest-spread",
    "transform-runtime",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-assign",
    "transform-object-rest-spread"
  ],
  "env": {
    "HOT": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
